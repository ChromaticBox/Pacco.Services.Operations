{
  "customers-service": {
    "namespace": "customers",
    "commands": [
      "create_customer"
    ],
    "events": [
      "customer_created"
    ],
    "rejectedEvents": [
      "create_customer_rejected"
    ]
  },
  "identity-service": {
    "namespace": "identity",
    "commands": [
      "sign_in",
      "sign_up"
    ],
    "events": [
      "signed_up",
      "signed_in"
    ],
    "rejectedEvents": [
      "sign_in_rejected",
      "sign_up_rejected"
    ]
  },
  "parcels-service": {
    "namespace": "parcels",
    "commands": [
      "add_parcel",
      "delete_parcel"
    ],
    "events": [
      "parcel_added",
      "parcel_deleted"
    ],
    "rejectedEvents": [
      "add_parcel_rejected",
      "delete_parcel_rejected"
    ]
  }
}